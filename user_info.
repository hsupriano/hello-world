fname = "First name: " + input("First name\n ")
lname = "Last name: " + input("Last name\n ")
age = "Years of age: " + input("Age\n ") 
dob = "Date of birth: " + input("Date of birth\n ")
address = "Street address: " + input("Street address\n ")
info= (fname + lname + age + dob + address)
with open ("user_info.txt", "w") as ui:
    ui.write(fname)
    ui.write("\n")
    ui.write(lname)
    ui.write("\n")
    ui.write(age)
    ui.write("\n")
    ui.write(dob)
    ui.write("\n")
    ui.write(address)
